<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Book_Vehicale"
    android:orientation="vertical"
    android:background="@drawable/cgb">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Book your Vehicle"
        android:textColor="#da9812"
        android:textColorHighlight="#000000"
        android:textSize="30sp"
        android:textStyle="bold|italic"
        android:layout_weight="1"
        />
    <TextView
        android:id="@+id/textView31"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="25dp"
        android:textColor="#DA9812"
        android:textSize="22sp"
        android:layout_marginTop="5dp"
        android:layout_weight="1"
        />

    <EditText
        android:id="@+id/editText17"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="50dp"
        android:layout_weight="1"
        android:background="@drawable/edittext_register"
        android:drawableLeft="@drawable/ic_account_circle_black_24dp"
        android:ems="10"
        android:gravity="center"
        android:hint="Your Name"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColor="#DA9812"
        android:textColorHint="#da9812"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/editText23"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="50dp"
        android:layout_weight="1"
        android:background="@drawable/edittext_register"
        android:drawableLeft="@drawable/ic_account_circle_black_24dp"
        android:ems="10"
        android:gravity="center"
        android:hint="Contact Number"
        android:inputType="phone"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColor="#DA9812"
        android:textColorHint="#da9812"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/editText12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="50dp"
        android:layout_weight="1"
        android:background="@drawable/edittext_register"
        android:drawableLeft="@drawable/ic_account_circle_black_24dp"
        android:ems="10"
        android:gravity="center"
        android:hint="Driving Liscence Number"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColor="#DA9812"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#da9812"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="50dp"
        android:layout_weight="1"
        android:background="@drawable/edittext_register"
        android:drawableLeft="@drawable/ic_access_time_black_24dp"
        android:ems="10"
        android:gravity="center"
        android:hint="Duaration(In Days)"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColor="#DA9812"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#da9812" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="50dp"
        android:layout_weight="1"
        android:background="@drawable/edittext_register"
        android:drawableLeft="@drawable/ic_account_circle_black_24dp"
        android:ems="10"
        android:gravity="center"
        android:hint="Pancard Number"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColor="#DA9812"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#da9812" />


    <EditText
        android:id="@+id/editText7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="50dp"
        android:layout_weight="1"
        android:background="@drawable/edittext_register"
        android:drawableLeft="@drawable/ic_account_circle_black_24dp"
        android:ems="10"
        android:gravity="center"
        android:hint="Aadharcard Number"
        android:inputType="phone"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColor="#DA9812"
        android:textColorHighlight="#da9812"
        android:textColorHint="#da9812" />





    <EditText
        android:id="@+id/editText26"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="50dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/edittext_register"
        android:drawableLeft="@drawable/ic_location_on_black_24dp"
        android:ems="10"
        android:gravity="center|center_horizontal"
        android:hint="Enter PickUp Point(Address)\n\n"
        android:inputType="textMultiLine"
        android:textColor="#DA9812"
        android:textColorHint="#DA9812"
        android:layout_gravity="center"
        android:layout_weight="1"/>


    <Button
        android:id="@+id/button4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/edittext_register"
        android:backgroundTint="#da9812"
        android:text="Book"
        android:textColor="#000000"
        android:layout_marginRight="40dp"
        android:layout_marginLeft="40dp"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="30dp"
        android:layout_weight="1"
        />

</LinearLayout>